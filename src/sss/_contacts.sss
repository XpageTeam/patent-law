.contacts
	&-list
		display: flex
		flex-wrap: wrap
		margin: 0 0 -30px -30px


.contact
	&_items
		margin-bottom: 30px

	&_item
		width: calc((100% - 30px * 3) / 3)
		margin: 0 0 30px 30px

		.contacts-el .title_city h3 
			font-size: 21px
			font-weight: 600
			margin-bottom: 25px

@media (max-width: 667px)

	.contact_item 
		width: 100%
